<script>
    var language = localStorage["lang"];
    $(function () {
        jQuery(".fancybox-fast-view").fancybox({
            'beforeClose': function () {
                $('#trailer').html('');
            }
        });
        //Click event handler
        $("#dangchieu").click(function () {
            //Web method post
            var aData = [];
            aData[0] = 1;
            var jsonData = JSON.stringify({ aData: aData });

            $.ajax({
                type: "POST",
                url: "Ajax.aspx/GetListFilmSpecialShowing",
                data: jsonData,
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                success: function (data) {
                    $('#tab-1').html(data.d);
                    jQuery(".fancybox-fast-view").fancybox({
                        'beforeClose': function () {
                            $('#trailer').html('');
                        }
                    });
                    jQuery(".btn-mua-ve2").fancybox({
                        'beforeClose': function () {
                            window.location.href = window.location.href;
                        }
                    });
                },
                error: function () {
                    alert("Error loading data! Please try again.");
                }
            })//done handler
                .done(function (msg) {
                    //Update the modal HTML
                    //alert("Done: " + msg.d);
                });
        });
        $("#sapchieu").click(function () {
            //Web method post
            var aData = [];
            aData[0] = 2;
