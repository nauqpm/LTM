    function viewTrailer(filmName, urlTrailer) {
        $('#tenphim').html(filmName.replace("&sv&", "\"").replace("&sv&", "\""));
        $('#trailer').html('<iframe id="fancybox-frame" width="100%" height="377" src="' + urlTrailer.replace("watch?v=", "embed/") + '?rel=0&amp;showinfo=0&amp;autoplay=1" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>');
    };
    function bookingSeat(tenRap, filmId, showId, stratTime, dateShow, filmName) {
        var urlBooking = (language == "vie" ? "/chon-ghe.htm?f=" : "/booking.htm?f=") + filmId + "&s=" + showId;
        $('#tenphim-datve').html(filmName.replace("&sv&", "\"").replace("&sv&", "\""));
        $('#rap').html('<span class="bold">' + tenRap + '</span>');
        $('#ngaychieu').html('<span class="bold">' + dateShow + '</span>');
        $('#giochieu').html('<span class="bold">' + stratTime + '</span>');
        $('#btndatve').attr("href", urlBooking);
        jQuery('.fancybox-dat-ve-modal-popup').fancybox({ afterLoad: function () { } }).trigger('click');
    };
    function tabClick(dt, filmGroupId, filmName, cinemaId) {
        var aData = [];
        aData[0] = dt;
        aData[1] = filmGroupId;
        aData[2] = filmName.replace("&sv&", "\"").replace("&sv&", "\"");
        aData[3] = cinemaId;

        var jsonData = JSON.stringify({ aData: aData });
        $.ajax({
            type: "POST",
            url: "Ajax.aspx/LoadShowByFilmAndDate",
            data: jsonData,
            dataType: "json",
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                $('#tab-content').html(data.d);
                jQuery(".fancybox-fast-view").fancybox({
                });
            },
            error: function () {
                alert("Error loading data! Please try again.");
            }
        })//done handler
            .done(function (msg) {
            });
    };
    function viewsShowtimes(cinemaId, filmGroupId, filmName, cinemaName) {
        $('#tenphim-showtimes').html(filmName.replace("&sv&", "\"").replace("&sv&", "\""));
        $('#tenrap-showtimes').html(cinemaName);
        $('#hdFilmId').val(filmGroupId);
        $('#tenphim-showtimes').val(filmName.replace("&sv&", "\"").replace("&sv&", "\""));
        var aData = [];
        aData[0] = cinemaId;
        aData[1] = filmGroupId;
        aData[2] = filmName.replace("&sv&", "\"").replace("&sv&", "\"");
        var jsonData = JSON.stringify({ aData: aData });

        $.ajax({
            type: "POST",
            url: "Ajax.aspx/LoadShowtimesByFilm",
            data: jsonData,
            dataType: "json",
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                $('#content-showtimes').html(data.d);
                jQuery(".phim-datve-pop-up").fancybox({
                });
                jQuery(".btn-mua-ve2").fancybox({
                    'beforeClose': function () {
                        window.location.href = window.location.href;
                    }
                });
            },
            error: function () {
                alert("Error loading data! Please try again.");
            }
        })//done handler
            .done(function (msg) {
                //Update the modal HTML
                //alert("Done: " + msg.d);
            });
    };
    function ChooseCinemaShowtimes(cinemaId, cinemaName) {
        var aData = [];
        aData[0] = cinemaId;
        aData[1] = cinemaName;
        aData[2] = $('#hdFilmId').val();
        aData[3] = $('#tenphim-showtimes').val();
        var jsonData = JSON.stringify({ aData: aData });
        $.ajax({
            type: "POST",
            url: "Ajax.aspx/ChooseCinemaShowtimes",
            data: jsonData,
            dataType: "json",
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                $('#defaultCinema').html(cinemaName);
                $('#tenrap-showtimes').html(cinemaName);
                $('#content-showtimes').html(data.d);
                jQuery(".phim-datve-pop-up").fancybox({
                });
            },
            error: function () {
                alert("Error loading data! Please try again.");
            }
        })//done handler
            .done(function (msg) {
            });
    };
</script>
